    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "extract-loader": "^2.0.1",
    "file-loader": "^2.0.0",
    "normalize.css": "^8.0.1",
    "postcss-cli": "^6.0.0",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.7",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.5.0",
    "stylelint-prettier": "^1.1.2",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
