{
  "name": "og-css",
  "description": "A css package built on top of tailwind.css with more utility classes.",
  "version": "3.1.0",
  "version_short": "3.0",
  "author": "Octan Group (https://octangrp.com)",
  "keywords": [
    "css",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octangrp/og-css.git"
  },
  "style": "dist/css/og.css",
  "sass": "sass/og.scss",
  "bugs": {
    "url": "https://github.com/octangrp/og-css/issues"
